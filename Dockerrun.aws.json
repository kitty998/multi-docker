{
    "AWSEBDockerrunVersion": 2, 
    "containerDefinitions": [
        {
            "name": "client",
            "image": "katarinacimesa/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "katarinacimesa/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "katarinacimesa/multi-worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "katarinacimesa/multi-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort":80
                }
            ],
            "links": ["client", "server"]
            
        }
    ]
}